{
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.fontSize": 14,
    "editor.fontFamily": "Hack Nerd Font, monospace",
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": true,
    "editor.tabCompletion": "on",
    "editor.suggestSelection": "first",
    "editor.wordWrap": "wordWrapColumn",
    "eslint.format.enable": true,
    "eslint.quiet": true,
    "eslint.run": "onSave",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/Pods": true
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "material-icon-theme.folders.theme": "specific",
    "terminal.external.osxExec": "Alacritty",
    "workbench.colorTheme": "Ayu Mirage",
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.statusBar.visible": true,
    "yaml.schemas": {
        "file:///Users/pope/.vscode/extensions/atlassian.atlascode-3.0.9/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "redhat.telemetry.enabled": false,
    "atlascode.bitbucket.enabled": false,
    "atlascode.jira.enabled": true,
    "atlascode.jira.jqlList": [
        {
            "id": "ef6a1535-583e-41f5-9e76-777aa2128e44",
            "enabled": true,
            "name": "My going Issues",
            "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
            "siteId": "455f06f1-e98b-485f-a563-aab65c13eeeb",
            "monitor": true
        }
    ],

}
